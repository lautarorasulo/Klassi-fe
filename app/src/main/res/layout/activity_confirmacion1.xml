<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".confirmacion1">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="90dp"
        tools:srcCompat="@tools:sample/avatars[0]" />

    <TextView
        android:id="@+id/p_cnf1_nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:text="@string/str_p_cnf1_nombre"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/p_cnf1_apellido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="84dp"
        android:text="@string/str_p_cnf1_apellido"
        app:layout_constraintEnd_toEndOf="@+id/p_cnf1_nombre"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/p_cnf1_nombre"
        app:layout_constraintTop_toBottomOf="@+id/p_cnf1_nombre" />

    <TextView
        android:id="@+id/p_cnf1_localidad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:text="@string/str_p_cnf1_localidad"
        app:layout_constraintEnd_toEndOf="@+id/p_cnf1_apellido"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toStartOf="@+id/p_cnf1_apellido"
        app:layout_constraintTop_toBottomOf="@+id/p_cnf1_apellido" />

    <Button
        android:id="@+id/btn_cnf1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="116dp"
        android:text="@string/str_p_cnf1btn"
        app:layout_constraintEnd_toEndOf="@+id/p_cnf1_localidad"
        app:layout_constraintStart_toStartOf="@+id/p_cnf1_localidad"
        app:layout_constraintTop_toBottomOf="@+id/p_cnf1_localidad" />
</android.support.constraint.ConstraintLayout>